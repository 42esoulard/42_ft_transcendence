{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": false,
          "introspectComments": false,
          "dtoFileNameSuffix": [".interface.ts", ".dto.ts", ".entity.ts"]
        }
      }
    ]
  }
}
